{
    "extends": "airbnb",
    "plugins": [
        "import"
    ],
    "rules": {
        "brace-style": ["error", "stroustrup"],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": "0",
        "jsx-a11y/anchor-is-valid": [ "error", { "components": [ "Link" ], "specialLink": [ "to" ] } ]    },
    "env": {
        "browser": true
    } 
}